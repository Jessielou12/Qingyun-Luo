if __name__ == '__main__':
    a = int(input())
    b = int(input())
    
def add(a, b):
    return a + b
    
def subtract(a, b):
    return a - b
    
def multiply(a, b):
    return a * b
    
def calculator():
    print(add(a, b))
    print(subtract(a, b))
    print(multiply(a, b))
    
calculator()
